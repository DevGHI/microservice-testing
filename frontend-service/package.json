{
  "name": "akh",
  "version": "1.2.2",
  "description": "React version of Argon Dashboard by Creative Tim",
  "main": "index.js",
  "keywords": [
    "react",
    "reactjs"
  ],
  "author": "Creative Tim",
  "license": "MIT",
  "homepage": "https://www.creative-tim.com/admin",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile:scss": "sass src/assets/scss/akh-gameshop.scss src/assets/css/akh-gameshop.css",
    "minify:scss": "sass src/assets/scss/akh-gameshop.scss src/assets/css/akh-gameshop.min.css --style compressed",
    "build:scss": "npm run compile:scss && npm run minify:scss && npm run map:scss"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.15.3",
    "axios": "^1.3.2",
    "bootstrap": "4.6.0",
    "chart.js": "2.9.4",
    "classnames": "2.3.1",
    "moment": "2.29.4",
    "nouislider": "15.2.0",
    "react": ">=16.0.0",
    "react-bootstrap-daterangepicker": "^8.0.0",
    "react-chartjs-2": "2.11.1",
    "react-copy-to-clipboard": "5.1.0",
    "react-datetime": "3.1.1",
    "react-dom": ">=16.0.0",
    "react-dropzone": "^14.2.3",
    "react-icons": "^4.7.1",
    "react-router-dom": "5.2.0",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.1.1",
    "reactstrap": "8.9.0",
    "sass": "1.54.5",
    "socket.io-client": "^4.6.1",
    "sweetalert2": "^11.7.1"
  },
  "devDependencies": {
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.9"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "optionalDependencies": {
    "eslint-plugin-flowtype": "8.0.3",
    "jquery": "3.6.0",
    "typescript": "4.8.2"
  }
}
